        },
        { "name": "waypoints",
          "type": "Waypoint",
          "isMany": true,
          "xml": { "serialize": "property" }
        }
      ]
    },
    {
      "name": "Bounds",
      "superClass": [
        "dc:Bounds",
        "Element"
      ]
    },
    {
      "name": "Waypoint",
      "superClass": [
        "dc:Point"
      ]
    }
  ]
}
