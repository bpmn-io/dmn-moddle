{
  "name": "bpmn.io DI for DMN",
  "uri": "http://bpmn.io/schema/dmn/biodi/1.0",
  "prefix": "biodi",
  "xml": {
    "tagAlias": "lowerCase"
  },
  "types": [
    {
      "name": "Edge",
      "superClass": [
        "Element"
      ],
      "properties": [
        { "name": "source",
