{
  "name": "dmn-moddle",
  "version": "10.0.0",
  "description": "A moddle wrapper for DMN 1.3",
  "files": [
    "dist",
    "resources",
    "!resources/dmn/xmi/"
  ],
  "scripts": {
    "all": "run-s generate-schema lint test distro",
    "lint": "eslint .",
    "dev": "npm test -- --watch",
    "test": "mocha --reporter=spec --recursive test/spec",
    "distro": "run-s build test:build",
    "build": "rollup -c",
    "test:build": "mocha --reporter=spec --recursive test/distro",
    "prepublishOnly": "run-s distro",
    "generate-schema": "node tasks/generate-schema.cjs",
    "test:tck": "npm run test:integration",
    "test:integration": "mocha --reporter=spec test/integration/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/dmn-moddle"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./resources/*": "./resources/*",
    "./package.json": "./package.json"
  },
  "keywords": [
    "dmn",
    "moddle",
    "meta-model"
  ],
  "author": {
    "name": "Sebastian Stamm",
    "url": "https://github.com/SebastianStamm"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "chai": "^4.3.7",
    "cmof-parser": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-plugin-bpmn-io": "^2.0.2",
    "execa": "^5.0.0",
    "glob": "^7.1.6",
    "jsondiffpatch": "^0.4.1",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.12.1",
    "sax": "^1.2.4",
    "tiny-stack": "^2.0.1",
    "xsd-schema-validator": "^0.7.0"
  },
  "dependencies": {
    "min-dash": "^4.0.0",
    "moddle": "^6.0.0",
    "moddle-xml": "^10.0.0"
  }
}
